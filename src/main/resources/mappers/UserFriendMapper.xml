<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="per.wilson.chat.mapper.UserFriendMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="per.wilson.chat.domain.entity.UserFriend">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="user_friend_id" property="userFriendId"/>
        <result column="friend_note" property="friendNote"/>
        <result column="friend_group_id" property="friendGroupId"/>
    </resultMap>

        <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, user_friend_id, friend_note, friend_group_id
    </sql>
</mapper>
